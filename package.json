{
  "name": "react-typescript-babel-demo",
  "version": "2.0.0",
  "description": "React typescript babel app demo.",
  "main": "index.js",
  "repository": "https://github.com/LaamGinghong/react-typescript-babel-demo.git",
  "author": "LaamGinghong <351390560@qq.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "cross-env-shell NODE_ENV=development ts-node --files -P tsconfig.json scripts/start.ts --open",
    "build": "cross-env-shell NODE_ENV=production ts-node --files -P tsconfig.json scripts/build.ts",
    "build-analyze": "cross-env-shell NODE_ENV=production ts-node --files -P tsconfig.json scripts/build.ts --analyze",
    "lint": "yarn lint-eslint && yarn lint-stylelint",
    "lint-eslint": "eslint -c .eslintrc.js --ext .ts, .tsx, .js src/**/*.{ts, tsx, js}",
    "lint-stylelint": "stylelint --config .stylelintrc src/**/*.less --syntax less",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "browserslist": {
    "production": [
      "iOS >= 8",
      "Android >= 5.0"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/*.{tsx,ts,js}": [
      "eslint -c .eslintrc.js"
    ],
    "./src/*.{less,css}": [
      "stylelint --config .stylelintrc"
    ],
    "./src/*.{ts,tsx,js,json,less,css,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/case-sensitive-paths-webpack-plugin": "^2.1.4",
